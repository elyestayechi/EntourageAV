{
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://entourageav-production.up.railway.app/api/$1",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS", "HEAD"],
        "headers": {
          "X-Forwarded-Host": "entourage-av.vercel.app"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }